[user]
	name = Eric Naeseth
	email = <%= prompt('Git committer email:', `git config --global user.email`) %>
[color]
	diff = auto
	status = auto
	branch = auto
	interactive = auto
[alias]
	ci = commit
	co = checkout
	branches = branch
	b = branch
	s = status
	cb = !git branch | egrep ^\\\\* | sed -e "'s/^\\* //'"
	lol = log --pretty=oneline
	lm = log --pretty=oneline master..HEAD
	ln = log --pretty=oneline next..HEAD
	lu = !git log --pretty=oneline origin/$(git cb)..HEAD
	lastweek = !git shortlog --after=\"`date -j -v-1w '+%b %d %Y'`\"
	update = !git stash && git pull --rebase && git stash pop
	crush = rebase -i --autosquash
	ff = merge --ff-only
	ship = !git pull --rebase && git push
	d = diff
	di = diff --cached
	cp = cherry-pick
[github]
	user = enaeseth
	token = <%= prompt('GitHub API token:', `git config --global github.token`) %>
[merge]
	tool = opendiff
[core]
	editor = mvim -f
[url "git@github.com:"]
	insteadOf = "gh:"
	pushInsteadOf = "github:"
	pushInsteadOf = "git://github.com/"
[url "git://github.com/"]
	insteadOf = "github:"
[url "git@gist.github.com:"]
	insteadOf = "gst:"
	pushInsteadOf = "gist:"
	pushInsteadOf = "git://gist.github.com/"
[url "git://gist.github.com/"]
	insteadOf = "gist:"
