[user]
	name = Eric Naeseth
	email = {email}
[color]
	diff = auto
	status = auto
	branch = auto
	interactive = auto
[alias]
	ci = commit
	co = checkout
	branches = branch
	b = branch
	s = status
	cb = rev-parse --abbrev-ref HEAD
	lol = !echo Did you mean: git ls?
	ls = log --pretty=format:"%C(yellow)%h\\ %Creset%s%Cblue\\ [%cn]" --decorate
	ll = log --pretty=format:"%C(yellow)%h\\ %Creset%s%Cblue\\ [%cn]" --decorate --stat
	ld = !echo but you know you really want lsd...
	lsd = log --pretty=format:"%C(yellow)%h\\ %C(green)%ad\\ %Creset%s%Cblue\\ [%cn]" --decorate --date=relative
	lf = log -u
	lm = log --pretty=format:"%C(yellow)%h\\ %Creset%s%Cblue\\ [%cn]" --decorate master..HEAD
	ln = log --pretty=format:"%C(yellow)%h\\ %Creset%s%Cblue\\ [%cn]" --decorate next..HEAD
	lu = !git log --pretty=format:'%C(yellow)%h %C(green)%ad %Creset%s%Cblue' --decorate --date=relative @{upstream}..HEAD
	lastweek = !git shortlog --after=\"`date -j -v-1w '+%b %d %Y'`\"
	update = !git stash && git pull --rebase && git stash pop
	crush = rebase -i --autosquash
	ff = merge --ff-only
	ship = !git pull --rebase && git push
	d = diff
	di = diff --cached
	cp = cherry-pick
	checkpoint = !git add -A && git commit -m Checkpoint
	ds = diff --staged
	pr = pull --rebase
	aliases = "!git config -l | grep alias | cut -c 7-"
[github]
	user = enaeseth
[merge]
	tool = opendiff
[core]
	editor = mvim -f
[url "git@github.com:"]
	insteadOf = "gh:"
	pushInsteadOf = "github:"
	pushInsteadOf = "git://github.com/"
[url "git://github.com/"]
	insteadOf = "github:"
[url "git@gist.github.com:"]
	insteadOf = "gst:"
	pushInsteadOf = "gist:"
	pushInsteadOf = "git://gist.github.com/"
[url "git://gist.github.com/"]
	insteadOf = "gist:"
[gist]
	private = yes
[stgit "alias"]
	submit = /src/thumbtack/python/thumbtack/tools/rietveld/stgit_submit.py
	approved = /src/thumbtack/python/thumbtack/tools/rietveld/stgit_issue_status.py
[stgit "submit"]
	uploader = /src/thumbtack/python/thumbtack/tools/code_review_upload.py
[push]
	default = upstream
[advice]
	statusHints = false
